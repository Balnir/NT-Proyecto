@model NT_Proyecto.Models.Account
@{
    ViewBag.Title = "Confirmacion de datos";
    Layout = "~/Views/Shared/_Layout.cshtml";
    @Scripts.Render("~/Content")
    @Scripts.Render("~/Scripts/js")
}


<div class="container">
    <h2>Creacion de Usuario</h2>
    <div class="jumbotron">
        <p>Felicitaciones @Html.DisplayFor( model => model.Nombre), @Html.DisplayFor( model => model.Apellido ). Usted se ha registrado exitosamente.</p>
        <p> Recuerde que para logear debe usar su correo electronico: @Html.DisplayFor( model => model.Email) y su contraseña.</p>
        <p>
            <a href="@Url.Action("Index", "Home" )">
                <button class="btn btn-primary btn-block">Volver al principio</button>
            </a>
        </p>
    </div>
</div>
